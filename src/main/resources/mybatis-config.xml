<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org/DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
    <!--  settings, typeAlias만 설정 (순서 지키기)  -->
    <settings>
        <setting name="jdbcTypeForNull" value="NULL"/>
    </settings>
    <typeAliases>
        <typeAlias type="com.my.spring.common.StringArrayTypeHandler" alias="strArr"/>
        <typeAlias type="com.my.spring.member.model.dto.Member" alias="member"/>
    </typeAliases>
</configuration>